    <script>
        // Smooth scrolling
        document.querySelectorAll('a[href^="#"]').forEach(anchor => {
            anchor.addEventListener('click', function (e) {
                e.preventDefault();
                const target = document.querySelector(this.getAttribute('href'));
                if (target) {
                    target.scrollIntoView({
                        behavior: 'smooth',
                        block: 'start'
                    });
                }
            });
        });

        // Navbar effect on scroll
        window.addEventListener('scroll', function() {
            const navbar = document.querySelector('.navbar');
            if (window.scrollY > 100) {
                navbar.style.background = 'rgba(255, 255, 255, 0.98)';
                navbar.style.boxShadow = '0 2px 30px rgba(168, 237, 234, 0.2)';
            } else {
                navbar.style.background = 'rgba(255, 255, 255, 0.95)';
                navbar.style.boxShadow = '0 2px 20px rgba(168, 237, 234, 0.15)';
            }
        });

        // Animate elements on scroll
        function animateOnScroll() {
            const elements = document.querySelectorAll('.animate-on-scroll');
            const windowHeight = window.innerHeight;

            elements.forEach(element => {
                const elementTop = element.getBoundingClientRect().top;
                const elementVisible = 150;

                if (elementTop < windowHeight - elementVisible) {
                    element.classList.add('animated');
                }
            });
        }

        // Calculator Functions
        function calculateBMI() {
            const weight = parseFloat(document.getElementById('bmi-weight').value);
            const height = parseFloat(document.getElementById('bmi-height').value);
            const resultElement = document.getElementById('bmi-result');
            
            if (weight > 0 && height > 0) {
                const bmi = weight / ((height / 100) * (height / 100));
                let category = '';
                
                if (bmi < 18.5) category = '×ª×ª ××©×§×œ';
                else if (bmi < 25) category = '××©×§×œ ×ª×§×™×Ÿ';
                else if (bmi < 30) category = '×¢×•×“×£ ××©×§×œ';
                else category = '×”×©×× ×”';
                
                resultElement.innerHTML = `×”-BMI ×©×œ×›×: <strong>${bmi.toFixed(1)}</strong><br>×§×˜×’×•×¨×™×”: <strong>${category}</strong>`;
                resultElement.classList.add('show');
            } else {
                resultElement.innerHTML = '×× × ×”×–×™× ×• ××©×§×œ ×•×’×•×‘×” ×ª×§×™× ×™×';
                resultElement.classList.add('show');
            }
        }

        function calculateCalories() {
            const weight = parseFloat(document.getElementById('cal-weight').value);
            const height = parseFloat(document.getElementById('cal-height').value);
            const age = parseFloat(document.getElementById('cal-age').value);
            const gender = document.getElementById('cal-gender').value;
            const activity = parseFloat(document.getElementById('cal-activity').value);
            const resultElement = document.getElementById('calories-result');
            
            if (weight > 0 && height > 0 && age > 0 && gender && activity) {
                let bmr;
                if (gender === 'male') {
                    bmr = 88.362 + (13.397 * weight) + (4.799 * height) - (5.677 * age);
                } else {
                    bmr = 447.593 + (9.247 * weight) + (3.098 * height) - (4.330 * age);
                }
                
                const calories = Math.round(bmr * activity);
                resultElement.innerHTML = `×”×¦×¨×™×›×” ×”×™×•××™×ª ×©×œ×›×:<br><strong>${calories}</strong> ×§×œ×•×¨×™×•×ª ×œ×™×•×`;
                resultElement.classList.add('show');
            } else {
                resultElement.innerHTML = '×× × ××œ××• ××ª ×›×œ ×”×©×“×•×ª';
                resultElement.classList.add('show');
            }
        }

        function calculateWater() {
            const weight = parseFloat(document.getElementById('water-weight').value);
            const activity = parseFloat(document.getElementById('water-activity').value);
            const weather = parseFloat(document.getElementById('water-weather').value);
            const resultElement = document.getElementById('water-result');
            
            if (weight > 0) {
                // Base calculation: 35ml per kg of body weight
                let waterNeeds = weight * 35;
                
                // Add for activity
                if (activity) {
                    waterNeeds += activity * 500; // Add 500ml per activity level
                }
                
                // Add for weather
                if (weather) {
                    waterNeeds += weather * 500; // Add 500ml per weather level
                }
                
                const liters = (waterNeeds / 1000).toFixed(1);
                const glasses = Math.round(waterNeeds / 250); // 250ml per glass
                
                resultElement.innerHTML = `×›××•×ª ×”××™× ×”××•××œ×¦×ª:<br><strong>${liters}</strong> ×œ×™×˜×¨ ×œ×™×•×<br>(×›-<strong>${glasses}</strong> ×›×•×¡×•×ª)`;
                resultElement.classList.add('show');
            } else {
                resultElement.innerHTML = '×× × ×”×–×™× ×• ××©×§×œ ×ª×§×™×Ÿ';
                resultElement.classList.add('show');
            }
        }

        function calculateIdealWeight() {
            const height = parseFloat(document.getElementById('ideal-height').value);
            const gender = document.getElementById('ideal-gender').value;
            const frame = document.getElementById('ideal-frame').value;
            const resultElement = document.getElementById('ideal-result');
            
            if (height > 0 && gender) {
                let idealWeight;
                
                if (gender === 'male') {
                    idealWeight = 50 + (2.3 * ((height - 152.4) / 2.54));
                } else {
                    idealWeight = 45.5 + (2.3 * ((height - 152.4) / 2.54));
                }
                
                // Adjust for frame size
                let adjustment = 0;
                if (frame === 'small') adjustment = -10;
                else if (frame === 'large') adjustment = 10;
                
                const minWeight = Math.round(idealWeight + adjustment - 5);
                const maxWeight = Math.round(idealWeight + adjustment + 5);
                
                resultElement.innerHTML = `×”×˜×•×•×— ×”××™×“×™××œ×™ ×©×œ×›×:<br><strong>${minWeight}-${maxWeight}</strong> ×§"×’<br>××©×§×œ ××™×“×™××œ×™: <strong>${Math.round(idealWeight + adjustment)}</strong> ×§"×’`;
                resultElement.classList.add('show');
            } else {
                resultElement.innerHTML = '×× × ×”×–×™× ×• ×’×•×‘×” ×•××™×Ÿ';
                resultElement.classList.add('show');
            }
        }

        // Contact WhatsApp function
        function contactWhatsApp(service = '') {
            let message = `×©×œ×•× ×œ×™×¨×•×Ÿ! ×× ×™ ××¢×•× ×™×™× /×ª `;
            if (service) {
                message += `×‘${service}. `;
            } else {
                message += `×‘×™×¢×•×¥ ×ª×–×•× ×”. `;
            }
            message += `××©××— ×œ×§×‘×œ ×¤×¨×˜×™× × ×•×¡×¤×™× ×•×œ×ª×× ×¤×’×™×©×ª ×”×™×›×¨×•×ª.`;
            
            const whatsappUrl = `https://wa.me/972523456789?text=${encodeURIComponent(message)}`;
            window.open(whatsappUrl, '_blank');
        }

        // Form submission
        function submitForm(event) {
            event.preventDefault();
            
            const name = document.querySelector('input[placeholder="××™×š × ×•×— ×œ×›× ×©××§×¨× ×œ×›×?"]').value;
            const phone = document.querySelector('input[placeholder="052-123-4567"]').value;
            const consultationType = document.querySelectorAll('select')[0].value;
            const goals = document.querySelector('textarea').value;
            const day = document.querySelectorAll('select')[1]?.value || '';
            const time = document.querySelectorAll('select')[2]?.value || '';

            let whatsappMessage = `×©×œ×•× ×œ×™×¨×•×Ÿ!\\n\\n`;
            whatsappMessage += `×”×©× ×©×œ×™ ${name}\\n`;
            whatsappMessage += `×˜×œ×¤×•×Ÿ: ${phone}\\n\\n`;
            
            if (consultationType) {
                const consultationNames = {
                    'weight-loss': '×™×¨×™×“×” ×‘××©×§×œ',
                    'sports-nutrition': '×ª×–×•× ×” ×¡×¤×•×¨×˜×™×‘×™×ª',
                    'pregnancy': '×ª×–×•× ×” ×‘×”×¨×™×•×Ÿ',
                    'children': '×ª×–×•× ×ª ×™×œ×“×™×',
                    'medical': '×ª×–×•× ×” ×¨×¤×•××™×ª',
                    'group': '×™×¢×•×¥ ×§×‘×•×¦×ª×™',
                    'general': '×™×¢×•×¥ ×›×œ×œ×™'
                };
                whatsappMessage += `××¢×•× ×™×™× /×ª ×‘: ${consultationNames[consultationType]}\\n\\n`;
            }

            if (goals) {
                whatsappMessage += `×”××˜×¨×•×ª ×©×œ×™: ${goals}\\n\\n`;
            }

            if (day && time) {
                const dayNames = {
                    'sunday': '×¨××©×•×Ÿ',
                    'monday': '×©× ×™',
                    'tuesday': '×©×œ×™×©×™',
                    'wednesday': '×¨×‘×™×¢×™',
                    'thursday': '×—××™×©×™'
                };
                const timeNames = {
                    'morning': '×‘×•×§×¨ (9:00-12:00)',
                    'afternoon': '×¦×”×¨×™×™× (12:00-16:00)',
                    'evening': '×¢×¨×‘ (16:00-19:00)'
                };
                whatsappMessage += `×–××Ÿ ××•×¢×“×£: ×™×•× ${dayNames[day]}, ${timeNames[time]}\\n\\n`;
            }

            whatsappMessage += `××©××— ×œ×ª×× ×¤×’×™×©×ª ×”×™×›×¨×•×ª ×—×™× ×!\\n×ª×•×“×” ğŸ`;

            const whatsappUrl = `https://wa.me/972523456789?text=${encodeURIComponent(whatsappMessage)}`;
            window.open(whatsappUrl, '_blank');

            alert('×”×”×•×“×¢×” × ×©×œ×—×”! ×× ×—× ×• ×¢×•×‘×¨×™× ×¢×›×©×™×• ×œ-WhatsApp ×œ×”×©×œ××ª ×”×ª×™××•× ğŸ');
        }

        // Initialize animations
        window.addEventListener('scroll', animateOnScroll);
        window.addEventListener('load', animateOnScroll);

        // Page load animation
        window.addEventListener('load', function() {
            document.body.style.opacity = '1';
        });

        document.body.style.opacity = '0';
        document.body.style.transition = 'opacity 0.5s ease';

        // Add floating animation to nutrition icons
